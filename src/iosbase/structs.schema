struct State  {
  BirthTxHash []byte
  Value int64
  Script string
}

struct TxInput {
   TxHash []byte
   UnlockScript string
   StateHash []byte
}

struct Tx {
   Version int32
   Recorder string
   Inputs []TxInput
   Outputs []State
   Time int64
}

struct BlockHead {
   Version int8
   SuperHash []byte
   TreeHash []byte
   Time    int64
}

struct Block {
   Version   int32
   SuperHash []byte
   Head      BlockHead
   Content   []byte
}